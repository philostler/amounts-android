<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="@color/black"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:topLeftRadius="8px" android:topRightRadius="8px"/>
            <gradient android:startColor="@color/darkOrange" android:endColor="@color/orange" android:angle="90"/>
            <stroke android:color="@color/orange" android:width="1px"/>
        </shape>
    </item>
</layer-list>